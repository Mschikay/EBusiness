var animal = "Meowsy"
var state = "like";

var data = {
		animal: animal,
		state: state};

$("#btn").click(function(){
	$.ajax({
		url:"showAnimal",
		type: "get",
	    data: JSON.stringify(data),
		contentType:"application/json",
		success: [
			function (result, status) {
				var string = "Meowsy is a " + result[species] + "that" +
				state + result[food];
				alert(string)
		}],
		error: [function (res) {
		}]
	});
});